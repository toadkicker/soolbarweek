  </svg>
</div>
<div class="scrollbox">
  <table class="locations table table-striped">

    <tbody>
    <% @locations.each do |location| %>
        <tr class="location-row" data-id="<%= location.id %>">
          <!--<td><%#= link_to image_tag(location.logo.url(:thumb)), location %></td>-->
          <td class="text-center align-middle">
            <%= location.marker_number %>
          </td>
          <td>
            <%= location_address(location) %>
          </td>
          <td><%= t("location.#{location.id}.description", default: location.description) %></td>

          <td>
            <ul class="list-group">
              <% location.events.order(:start_time).each do |event| %>
                  <li>
                    <%= link_to t("event.#{event.id}.title", default: event.title), event_path(event) %>&nbsp;&ndash;&nbsp;<%= l(event.start_time, format: '%A %d, %l:%M %p') %>
                  </li>
              <% end %>
            </ul>
          </td>
          <% if user_is_admin? %>
              <td><%= link_to 'Edit', edit_location_path(location) %></td>
              <td><%= link_to 'Destroy', location, method: :delete, data: {confirm: 'Are you sure?'} %></td>
          <% end %>
        </tr>
    <% end %>
    </tbody>
  </table>
</div>

<%= content_for :head do %>
    <%= content_tag :script do %>
        <%= render 'locations/markers.js' %>
    <% end %>
<% end %>